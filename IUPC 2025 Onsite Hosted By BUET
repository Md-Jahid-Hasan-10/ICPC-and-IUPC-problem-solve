/*
auto Goriber_function = [&] (int m) -> int{

};
*/

const ll oo = 1e18;
const ll mod = 1e9 + 7;
const ll N = 2e5 + 123;

void Goriber_solve()
{
    int n,m,k;
    cin >> n >> k >> m;
    string s;
    cin >> s;
    deque<pii> dq;
    queue<pii> food;
    for(int i = 0; i < m; i++){
        int x,y;
        cin >> x >> y;
        food.push({x,y});
    }
    map<pii,int> bd;
    bd[{1,1}] = 1;
    dq.push_front({1,1});
    int ans = 1;
    for(int i = 0; i < k; i++){
        pii head = dq.front();
        pii tail = dq.back();
        if(s[i] == 'R'){
            head.S++;
            if(head.S == n + 1){
                head.S = 1;
            }
        }
        if(s[i] == 'L'){
            head.S--;
            if(head.S == 0)head.S = n;
        }
        if(s[i] == 'U'){
            head.F--;
            if(head.F == 0)head.F = n;
        }
        if(s[i] == 'D'){
            head.F++;
            if(head.F == n + 1)head.F = 1;
        }
        if(sz(food)){
            pii fo = food.front();
            if(fo.F == head.F && fo.S == head.S){
                food.pop();
                ans++;
            }
            else{
                bd[tail] = 0;
                if(bd[head] == 1){
                    cout << "DEAD" << endl;
                    return;
                }
                dq.pop_back();
            }
        }
        else{
            bd[tail] = 0;
            if(bd[head] == 1){
                cout << "DEAD" << endl;
                return;
            }
            dq.pop_back();
        }
        bd[head] = 1;
        dq.push_front(head);
    }
    cout << "ALIVE" << " " << ans << endl;
}

signed main()
{
    Fokinnir_moto_kaj_koris_na
    int _ = 1,tc = 0;
    //cin >> _;
    while (_--)
    {
        //cout<<"Case "<<++tc<<": ";
        Goriber_solve();
    }
    return 0;
}

    
