typedef long long ll;
typedef double dl;
typedef vector<int> vi;
typedef vector<vi> vvi;
typedef pair<int, int> pii;
typedef vector<pii> vii;
typedef array<int,3> tu;

const ll oo = 1e18;
const ll mod = 1e9 + 7;
const ll N = 2e5 + 123;

void Goriber_solve()
{
    int n,m;
    cin >> n >> m;
    int ar[n+5][m+5];
    for(int i = 1; i <= n; i++){
        for(int j = 1; j <= m; j++){
            cin >> ar[i][j];
        }
    }
    vi row(n+5,0),col(m+5,0);
    for(int i = 1; i <= n; i++){
        int xr = 0;
        for(int j = 1; j <= m; j++){
            xr ^= ar[i][j];
        }
        row[i] = xr;
    }
    for(int j = 1; j <= m; j++){
        int xj = 0;
        for(int i = 1; i <= n; i++){
            xj ^= ar[i][j];
        }
        col[j] = xj;
    }
    int ans = 0,mn = 0;
    for(int i = 1; i <= n; i++)ans += row[i];
    for(int j = 1; j <= m; j++)ans += col[j];
    for(int i = 1; i <= n; i++){
        for(int j = 1; j <= m; j++){
            int x1 = row[i],x2 = col[j];
            int pp = 0;
            for(int k = 0; k < 32; k++){
                if(getbit(x1,k) == getbit(x2,k) && getbit(x1,k) == 1){
                    pp += (1LL << k);
                }
            }
            //cout << pp << " ";
            mn = max(mn,2 * pp);
        }
        //cout << endl;
    }
    cout << ans  - mn << endl;
}
signed main()
{
    Fokinnir_moto_kaj_koris_na
    int _ = 1,tc = 0;
    cin >> _;
    while (_--)
    {
        //cout<<"Case "<<++tc<<": ";
        Goriber_solve();
    }
}
