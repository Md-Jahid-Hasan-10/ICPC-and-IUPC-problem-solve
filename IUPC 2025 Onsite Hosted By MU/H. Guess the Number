const ll oo = 1e15;
const ll mod = 1e9 + 7;
const ll N = 1008;
int gese[N];
vector<int> primes;
bitset<N>is_prime;

void sieve_v0(){
  for(int i = 3; i < N; i += 2){
    is_prime[i] = 1;
  }
  for(int i = 3; i * i < N; i += 2){
    if (is_prime[i]) {
      for (int j = i * i; j < N; j += i) {
        is_prime[j] = 0;
      }
    }
  }
  is_prime[2] = 1;
  for(int i = 2; i < N; i++){
    if(is_prime[i]){
      primes.push_back(i);
    }
  }
}
char f(int x){
    cout << "? " << x << endl;
    //cout << endl;
    fflush(stdout);
    // if(x % 557 == 0)return 'B';
    // return 'A';
    char ch;
    cin >> ch;
    return ch;
}
vii ff(int lc){
    vii pp;
    for(auto u : primes){
        if(lc % u == 0){
            int cnt = 0;
            while(lc % u == 0){
                lc /= u;
                cnt++;
            }
            pp.pb({u,cnt});
        }
    }
    return pp;
}
void Goriber_solve()
{
    for(int i = 0; i <= 1001; i++){
        gese[i] = 0;
    }
    int lc = 1;
    for(int i = 1001; i >= 1; i--){
        if(gese[i] == 1)continue;
        lc = lcm(lc,i);
        if(lc <= oo)continue;
        char ch = f(lc);
        if(ch == 'A'){
            for(int j = 1; j <= i; j++){
                if(lc % j == 0){
                    gese[j] = 1;
                }
            }
            lc = 1;
            //cout << i << " i " << endl;
        }
        else{
            vii pp = ff(lc);
            int lala = lc;
            //cout << lala << " lala " << endl;
            
            // for(auto u : pp){
            //     cout << u.F << " " << u.S << endl;
            // }
            int ans = 1,val = lala;
            for(auto u : pp){
                int hh = 0;
                for(int j = u.S; j >= 1; j--){
                    val /= u.F;
                    char ss = f(val);
                    if(ss == 'A'){
                        val *= u.F;
                        break;
                    }
                    hh++;
                }
                //vii qq = ff(val);
                // cout << "after " << u.F << endl;
                // for(auto u : qq){
                //     cout << u.F << " " << u.S << endl;
                // }
            }
            vii de = ff(val);
            for(auto u : de){
                for(int j = 1; j <= u.S; j++){
                    ans *= u.F;
                }
            }
            if(ans != 1)ans--;
            cout << "! " << ans << endl;
            fflush(stdout);
            return;
        }
    }
    cout << "! " << 1 << endl;
    fflush(stdout);

}
signed main()
{
    Fokinnir_moto_kaj_koris_na
    sieve_v0();
    int _ = 1,tc = 0;
    cin >> _;
    while (_--)
    {
        //cout<<"Case "<<++tc<<": ";
        Goriber_solve();
    }
}
