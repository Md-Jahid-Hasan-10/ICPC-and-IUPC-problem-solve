typedef long long ll;
typedef double dl;
typedef vector<int> vi;
typedef vector<vi> vvi;
typedef pair<int, int> pii;
typedef vector<pii> vii;
typedef array<int,3> tu;

ll gcd(ll a, ll b) { return __gcd(a, b); }
ll lcm(ll a, ll b) { return a * (b / gcd(a, b)); }

/*
auto Goriber_function = [&] (int m) -> int{

};
*/

const ll oo = 1e18;
const ll MOD = 998244353;
const int MAXA = 500000 + 5;

ll bit[MAXA], a[MAXA], fact[MAXA];
bool is_inf[MAXA];
int n;

void add(int i, ll v){
    v = (v % MOD + MOD) % MOD;
    for (; i <= n; i += i & -i)
        bit[i] = (bit[i] + v) % MOD;
}

ll sum(int i){
    ll s = 0;
    for(; i > 0; i -= i & -i)
        s = (s + bit[i]) % MOD;
    return s;
}

void precompute(){
    fact[0] = 1;
    for(int i = 1; i < MAXA; i++)
        fact[i] = fact[i - 1] * i % MOD;
}

void Goriber_solve()
{
    int q;
    cin >> n >> q;
    memset(bit, 0, sizeof(ll) * (n + 2));
    set<int> active;
    for(int i = 1; i <= n; i++){
        cin >> a[i];
        is_inf[i] = false;
        add(i, a[i]);
        if(a[i] == 0 || a[i] > 2)active.insert(i);
    }
    while(q--){
        int type, l, r;
        cin >> type >> l >> r;
        if(type == 1){
            auto it = active.lower_bound(l);
            while(it != active.end() && *it <= r){
                int idx = *it;
                ll old = a[idx],nxt;
                bool next_inf = false;
                if(is_inf[idx]){
                    nxt = 0;
                    next_inf = true;
                }
                else if(old == 3628800){
                    nxt = 821984089;
                    next_inf = true;
                }
                else if(old == 39916800){
                    nxt = 644056242;
                    next_inf = true;
                }
                else if(old == 479001600){
                    nxt = 527656359;
                    next_inf = true;
                }
                else if(old >= MAXA){
                    nxt = 0;
                    next_inf = true;
                }
                else{
                    nxt = fact[old];
                    next_inf = (old >= 13);
                }
                add(idx, nxt - old);
                a[idx] = nxt;
                is_inf[idx] = next_inf;
                bool stable = (is_inf[idx] && a[idx] == 0) ||
                    (!is_inf[idx] && (a[idx] == 1 || a[idx] == 2));
                if(stable)it = active.erase(it);
                else ++it;
            }
        }
        else{
            ll ans = (sum(r) - sum(l - 1) + MOD) % MOD;
            cout << ans << endl;
        }
    }
}

signed main()
{
    Fokinnir_moto_kaj_koris_na
    precompute();
    int _ = 1,tc = 0;
    cin >> _;
    while (_--)
    {
        //cout<<"Case "<<++tc<<": ";
        Goriber_solve();
    }
    return 0;
}

    
