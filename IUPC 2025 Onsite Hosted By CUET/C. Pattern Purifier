const ll oo = 1e18;
const ll mod = 1e9 + 7;
const ll N = 2e5 + 123;

void Goriber_solve()
{
    string s;
    cin >> s;
    int n = sz(s);
    set<int> st;
    for(int i = 0; i < n; i++){
        st.insert(i);
    }
    for(int i = 0; i+1 < n; i++){
        if(s[i] == s[i+1]){
            if(st.count(i) && st.count(i+1)){
                st.erase(i);
                st.erase(i+1);
            }
        }
        if(st.count(i)){
            auto it = st.lower_bound(i);
            auto it2 = it;
            if(it2 != st.begin()){
                it2--;
            }
            if(*it != *it2 && s[*it] == s[*it2]){
                st.erase(*it);
                st.erase(*it2);
            }
        }
    }
    if(sz(st) == 2){
        auto it = st.begin();
        auto it2 = it;
        it2++;
        if(*it != *it2 && s[*it] == s[*it2]){
            st.erase(*it);
            st.erase(*it2);
        }
    }
    if(sz(st) == 0)py;
    else pn;
}

signed main()
{
    Fokinnir_moto_kaj_koris_na
    int _ = 1,tc = 0;
    cin >> _;
    while (_--)
    {
        //cout<<"Case "<<++tc<<": ";
        Goriber_solve();
    }
    return 0;
}

    
