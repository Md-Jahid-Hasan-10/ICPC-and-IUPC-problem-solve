const ll oo = 1e18;
const ll mod = 1e9 + 7;
const ll N = 2e5 + 123;

struct Node{
    Node* links[2];
    int cnt[2] = {0};
    bool contains(int bit){
        return (links[bit] != NULL);
    }
    void put(int bit, Node* node){
        links[bit] = node;
    }
    Node* get(int bit){
        return links[bit];
    }
    void cntIncrement(int bit){
        cnt[bit]++;
    }
    void cntDecrement(int bit){
        cnt[bit]--;
    }
    int getCnt(int bit){
        return cnt[bit];
    }
    void deleteNode() {
        for (int i = 0; i < 2; ++i) {
            if (links[i] != nullptr) {
                links[i]->deleteNode();
                delete links[i];
            }
        }
    }
};
class Trie{
private:
    Node *root;

public:
    Trie(){
        root = new Node();
    }
    void insert(int num){
        Node* cur_node = root;
        for(int i = 31; i >= 0; i--){
            int bit = (num >> i) & 1;
            if(!cur_node -> contains(bit)){
                cur_node -> put(bit, new Node());
            } 
            cur_node -> cntIncrement(bit);
            cur_node = cur_node -> get(bit);
        }
    }
    void remove(int num){
        Node* cur_node = root;
        for(int i = 31; i >= 0; i--){
            int bit = (num >> i) & 1;
            cur_node -> cntDecrement(bit);
            cur_node = cur_node -> get(bit);
        }
    }
    int getMax(int num){
        Node* cur_node = root;
        int mx = 0;
        for(int i = 31; i >= 0; i--){
            int bit = (num >> i) & 1;
            if(cur_node -> contains(bit) && cur_node->getCnt(bit) > 0){
                cur_node = cur_node -> get(bit);
            }
            else{
                mx = ((1LL << i) | mx);
                cur_node = cur_node -> get(bit ^ 1);
            }
        }
        return mx;
    }
    void deleteTrie() {
        root->deleteNode();
        delete root;
    }
};

void Goriber_solve()
{
    int n;
    cin >> n;
    vi ar(n);
    for(auto &i : ar)cin >> i;
    Trie trie;
    for(auto u : ar)trie.insert(u);
    int mn = oo;
    for(auto u : ar){
        trie.remove(u);
        mn = min(mn,trie.getMax(u));
        trie.insert(u);
    }
    cout << mn << endl;
    trie.deleteTrie();
}

signed main()
{
    Fokinnir_moto_kaj_koris_na
    int _ = 1,tc = 0;
    cin >> _;
    while (_--)
    {
        //cout<<"Case "<<++tc<<": ";
        Goriber_solve();
    }
    return 0;
}

    
